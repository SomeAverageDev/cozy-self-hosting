{
  "name": "self-hosting",
  "icon-path": "client/app/assets/icons/main_icon.svg",
  "version": "1.1.0",
  "engines": [
    "node >= 0.10.0"
  ],
  "description": "An application to manage self hosted application on an Debian",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cozy/cozy-self-hosting.git"
  },
  "main": "server.js",
  "dependencies": {
    "babel": "^5.8.23",
    "americano": "0.4.2",
    "cozydb": "0.1.3",
    "printit": "^0.1.3"
  },
  "devDependencies": {
    "should": "7.1.1",
    "mocha": "2.3.3",
    "request-json": "0.5.3",
    "cozy-fixtures": "1.0.1",
    "jslint": "0.9.3"
  },
  "scripts": {
    "test": "mocha tests/*.js",
    "start": "babel-node server.es6",
    "build": "make server"
  },
  "cozy-permissions": {
    "CozyInstance": {
      "description": "Used to read your cozy's domain"
    }
  },
  "cozy-displayName": "Self hosting"
}
