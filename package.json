{
  "name": "self-hosting",
  "icon-path": "client/app/assets/icons/main_icon.svg",
  "version": "2.1.0",
  "engines": [
    "node >= 0.10.0"
  ],
  "description": "An application to manage self hosted application on an Debian",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cozy/cozy-self-hosting.git"
  },
  "main": "server.js",
  "dependencies": {
    "americano": "0.4.2",
    "cozydb": "0.1.3",
    "printit": "^0.1.3"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "cozy-fixtures": "1.0.1",
    "jslint": "0.9.3",
    "mocha": "2.3.3",
    "request-json": "0.5.3",
    "should": "7.1.1"
  },
  "scripts": {
    "test": "mocha tests/*.js",
    "start": "node server.js"
  },
  "cozy-permissions": {
    "CozyInstance": {
      "description": "Used to read your cozy's domain"
    }
  },
  "cozy-displayName": "Self hosting"
